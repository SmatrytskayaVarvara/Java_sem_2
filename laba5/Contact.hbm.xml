<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="varia.by.Contact" table="address_book" >
        <id name="contact_id">
            <column name="contact_id" sql-type="int(11)"/>
        </id>
        <property name="first_name">
            <column name="first_name" sql-type="varchar(45)" length="50" not-null="true"/>
        </property>
        <property name="last_name">
            <column name="last_name" sql-type="varchar(45)" length="50" not-null="true"/>
        </property>
        <property name="address">
            <column name="address" sql-type="varchar(45)" not-null="true"/>
        </property>
        <property name="telephone">
            <column name="telephone" sql-type="varchar(45)" not-null="true"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(45)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>
